<div class="breadcrumb-wrap   bg-f br-3" style="margin-top:200px">
    <div class="container">
        <div class="breadcrumb-title">
            <h1>@ViewBag.Title</h1>
            <ul class="breadcrumb-menu list-style">
                <li><a href="/" title="@ViewBag.Title" rel="nofollow">خانه </a></li>
                @if (ViewBag.StatusLocationBarOne)
                {
                    <li><a href="#">@ViewBag.LocationBarOne</a></li>
                }
              
                <li>@ViewBag.Title</li>
            </ul>
        </div>
    </div>
</div>