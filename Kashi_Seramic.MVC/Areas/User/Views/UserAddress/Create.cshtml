@using Kashi_Seramic.MVC.Models.Users.UserAddres;



@model CreateUserAddressVM

@{
    ViewBag.title = " ثبت دسته جدید";
    ViewBag.keywords = "آموزشکده آرایشگران,اموزشگاه آرایش,آرایشگر,نمونه کار,ام البنین صفری,بیوتی صفری,صفری,ساری,مازندران,مازندران ساری,ایران,آرایشگران ";
    ViewBag.author = "ام البنین صفری";
    ViewBag.description = "-با میدیریت صفری من  ام البنبن صفری هستم  و برند صفری بیوتی در اختیار من است آموزشگاه آرایشگری به صورت حضوری و مجازی برگزار میشود با مدرک فنی حرفه ای";
    ViewBag.robot = "noindex,nofollow";
    ViewBag.socialmedia = false;
    ViewBag.link = "https://www.omolbininsafaribeauty.com";
    ViewBag.imageurl = "https://www.omolbininsafaribeauty.com/";
    ViewBag.username = "noosignalls";
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="col-md-12" dir="rtl">
    <div class="card" dir="rtl">
        <div class="card-header">
            <h3 class="card-title">دسته بندی جدید</h3>
        </div>
        <div class="card-body pb-2 ">




            <form class="needs-validation was-validated" action="/User/UserAddress/Create" method="post" enctype="multipart/form-data">
                <div class="row row-sm">
                    <div class="col-lg-12">
                        <div class=" row">
                            <label class=" col-md-2">عنوان</label>
                            <input name="Model.Title" class="form-control col-md-10 mb-4 is-invalid state-invalid" type="text" required />
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class=" row">
                            <label class=" col-md-2">نام</label>
                            <input name="Model.Name" class="form-control col-md-10 mb-4 is-invalid state-invalid" type="text" required />
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class=" row">
                            <label class=" col-md-2">نام خوانوادگی</label>
                            <input name="Model.Family" class="form-control col-md-10 mb-4 is-invalid state-invalid" type="text" required />
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class=" row">
                            <label class=" col-md-2">استان</label>
                            <input name="Model.Ostan" class="form-control col-md-10 mb-4 is-invalid state-invalid" type="text" required />
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class=" row">
                            <label class=" col-md-2">شهر</label>
                            <input name="Model.City" class="form-control col-md-10 mb-4 is-invalid state-invalid" type="text" required />
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class=" row">
                            <label class=" col-md-2">خیابان</label>
                            <input name="Model.Avenu" class="form-control col-md-10 mb-4 is-invalid state-invalid" type="text" required />
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class=" row">
                            <label class=" col-md-2">آدرس</label>

                            <input type="text"  name="Model.Address"  class="form-control col-md-10 mb-4 is-invalid state-invalid" required>



                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class=" row">
                            <label class=" col-md-2">تلفن</label>
                            <input name="Model.Phone" class="form-control col-md-10 mb-4 is-invalid state-invalid" type="text" required />
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class=" row">
                            <label class=" col-md-2">کد پستی</label>
                            <input name="Model.CodePostal" class="form-control col-md-10 mb-4 is-invalid state-invalid" type="text" required />
                        </div>
                    </div>
                    <input type="hidden" value="false" name="Model.ActiveByDefualt" />
                    <input type="submit" class="btn btn-success col-md-2 mt-3" value="ثبت" />
                    <a onclick="history.back()" class="btn btn-danger col-md-2 mt-3 mx-2">انصراف</a>

                </div>
            </form>
        </div>
    </div>
</div>
